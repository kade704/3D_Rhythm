        <arguments>
          <argument>
            <kind>1</kind>
            <string>///노트 판정

if(obj_JukeBox.IsPlaying)
{
    var key = keyboard_check_pressed(vk_anykey);
    
    var lastNoteTime = obj_Data.Timeline[Index - 1];
    var currentNoteTime = obj_Data.Timeline[Index];
    
    if((lastNoteTime &lt; current_time - TimeStart) &amp;&amp; (current_time - TimeStart &lt; lastNoteTime + JudgementRate) &amp;&amp; Index &gt; 1)
    {
        if(key &amp;&amp; IsHitted == false)
        {
            IsHitted = true;
            //create_effect(Index - 1);
        }
    }
    
    if((lastNoteTime + JudgementRate &lt; current_time - TimeStart) &amp;&amp; (current_time - TimeStart &lt; currentNoteTime - JudgementRate))
    {
        if(IsHitted == false)
        {
            game_restart();
        }
        if(key)
        {
            game_restart();
        }
        IsHitted = noone;
    }
    
    if((currentNoteTime - JudgementRate &lt; current_time - TimeStart) &amp;&amp; (current_time - TimeStart &lt; currentNoteTime))
    {
        if(IsHitted == noone)
        {
            IsHitted = false;
        }
        if(key &amp;&amp; IsHitted == false)
        {
            IsHitted = true;
            //create_effect(Index);
        }
    }
}
</string>
