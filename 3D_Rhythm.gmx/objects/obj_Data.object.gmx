        var u = rotate_axis(Ups[i - 1, 0], Ups[i - 1, 1], Ups[i - 1, 2], Rights[i - 1, 0], Rights[i - 1, 1], Rights[i - 1, 2], angle);
        Ups[i, 0] = u[0];
