            <string>///위치 변형하기

var index = obj_MainCube.Index;

var px = obj_MainData.Positions[index, 0];
var py = obj_MainData.Positions[index, 1];
var pz = obj_MainData.Positions[index, 2];

var fx = obj_MainData.Forwards[index, 0];
var fy = obj_MainData.Forwards[index, 1];
var fz = obj_MainData.Forwards[index, 2];

var rx = obj_MainData.Rights[index, 0];
var ry = obj_MainData.Rights[index, 1];
var rz = obj_MainData.Rights[index, 2];

var ux = obj_MainData.Ups[index, 0];
var uy = obj_MainData.Ups[index, 1];
var uz = obj_MainData.Ups[index, 2];

TargetX = lerp(TargetX, px, SmoothSpeed);
TargetY = lerp(TargetY, py, SmoothSpeed);
TargetZ = lerp(TargetZ, pz, SmoothSpeed);

PositionX = lerp(PositionX, px - (fx * Scale) + (rx * Scale) + (ux * Scale), SmoothSpeed);
PositionY = lerp(PositionY, py - (fy * Scale) + (ry * Scale) + (uy * Scale), SmoothSpeed);
PositionZ = lerp(PositionZ, pz - (fz * Scale) + (rz * Scale) + (uz * Scale), SmoothSpeed);

UpX = lerp(UpX, ux, SmoothSpeed);
UpY = lerp(UpY, uy, SmoothSpeed);
UpZ = lerp(UpZ, uz, SmoothSpeed);

Scale = lerp(Scale, 80, SmoothSpeed);
