var sz = obj_Data.Sides[index, 2];

var ux = obj_Data.Ups[index, 0];
var uy = obj_Data.Ups[index, 1];
var uz = obj_Data.Ups[index, 2];

var d = rotate_axis(dx, dy, dz, sx, sy, sz, -45);
d = rotate_axis(d[0], d[1], d[2], ux, uy, uz, 135);

TargetX = lerp(TargetX, px, SmoothSpeed);
TargetY = lerp(TargetY, py, SmoothSpeed);
TargetZ = lerp(TargetZ, pz, SmoothSpeed);

PositionX = lerp(PositionX, px + (d[0] * Scale), SmoothSpeed);
PositionY = lerp(PositionY, py + (d[1] * Scale), SmoothSpeed);
PositionZ = lerp(PositionZ, pz + (d[2] * Scale), SmoothSpeed);

NormalX = lerp(NormalX, ux, SmoothSpeed);
NormalY = lerp(NormalY, uy, SmoothSpeed);
NormalZ = lerp(NormalZ, uz, SmoothSpeed);
</string>
