var index = obj_Cube.Index;

var px = obj_Data.Positions[index, 0];
var py = obj_Data.Positions[index, 1];
var pz = obj_Data.Positions[index, 2];

var u = cross(obj_Data.Vectors[index, 0], obj_Data.Vectors[index, 1], obj_Data.Vectors[index, 2], obj_Data.Normals[index, 0], obj_Data.Normals[index, 1], obj_Data.Normals[index, 2]);
